SyncState *
# {{{ COLBY
IMAPAccount colby
# Address to connect to
Host imap.gmail.com
User gstell23@colby.edu
PassCmd "cat /etc/smtpd/secrets | awk -F: '/colbymail/{ print $2; }'"
# To store the password in an encrypted file use PassCmd instead of Pass
# PassCmd "gpg2 -q --for-your-eyes-only --no-tty -d ~/.mailpass.gpg"
#
# Use SSL
SSLType IMAPS
# The following line should work. If get certificate errors, uncomment the two following lines and read the "Troubleshooting" section.
CertificateFile /etc/ssl/certs/ca-certificates.crt
#CertificateFile ~/.cert/imap.gmail.com.pem
#CertificateFile ~/.cert/Equifax_Secure_CA.pem

IMAPStore colby-remote
Account colby

MaildirStore colby-local
Subfolders Verbatim
# The trailing "/" is important
Path ~/mail/colby/
Inbox ~/mail/colby/inbox

Channel colby-default
Far :colby-remote:
Near :colby-local:
# Exclude everything under the internal [Gmail] folder, except the interesting folders
# Patterns * ![Gmail]* "[Gmail]/Sent Mail" "[Gmail]/All Mail"
Patterns "INBOX"
# Or include everything
#Patterns *
# Automatically create missing mailboxes, both locally and on the server
Create Near

Channel colby-sent
Far :colby-remote:"[Gmail]/Sent Mail"
Near :colby-local:"sent"
Create Near

Channel colby-all
Far :colby-remote:"[Gmail]/All Mail"
Near :colby-local:"all"
Create Near

Channel colby-archive
Far :colby-remote:"Archived Mail"
Near :colby-local:"archive"
Create Near

Channel colby-intl
Far :colby-remote:"International"
Near :colby-local:"intl"
Create Near

Channel colby-gdrive
Far :colby-remote:"GDrive"
Near :colby-local:"gdrive"
Create Near

Channel colby-logins
Far :colby-remote:"Logins"
Near :colby-local:"logins"
Create Near

Channel colby-classes
Far :colby-remote:"Classes"
Near :colby-local:"classes"
Create Near

Channel colby-trash
Far :colby-remote:"[Gmail]/Trash"
Near :colby-local:"trash"
Create Near

Channel colby-spam
Far :colby-remote:"[Gmail]/Spam"
Near :colby-local:"spam"
Create Near

Channel colby-drafts
Far :colby-remote:"[Gmail]/Drafts"
Near :colby-local:"drafts"
Create Both

Group colby
Channel colby-default
Channel colby-sent
Channel colby-all
Channel colby-logins
Channel colby-classes
Channel colby-gdrive
Channel colby-drafts
Channel colby-spam
Channel colby-trash

# }}}

