
#vim ft=sh
# prints the names of all files which do not start with a commented header
# i.e. their first line does not start with /**
cs_find_uncommented () {
    find -type f | 
        xargs -I%% bash -c "cat %% | awk '( NR == 1) {print \$1 \"\t%%\"}' " | 
        grep -v '^/\*\*'
}

