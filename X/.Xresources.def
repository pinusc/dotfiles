! Do not edit .Xresources but .Xresources.def
! Rebuild with cpp -P .Xresources.def .Xresources
URxvt*termName:xterm-256color
URxvt*geometry:120*80
Xft.dpi: 80
URxvt*scrollBar:false
! URxvt*font:xft:Source Code Pro for Powerline:size=10
!URxvt*font:-gohu-*-medium-*-*-*-11-*-*-*-*-*-*-*
 URxvt*font: \
    -gohu-*-medium-*-*-*-14-*-*-*-*-*-*-*, \
    xft: Source Code Pro:style=Italic:size=14, \
    xft: Symbola:size=11:antialias=false

URxvt*iso14755:true
URxvt.imLocale:en_US.UTF-8
Xcursor.theme: Breeze
xterm*dynamicColors: true
urxvt*dynamicColors: on
URxvt.inputMethod: ibus
URxvt.preeditType: OverTheSpot
URxvt.internalBorder: 5
URxvt.perl-ext-common: default,font-size
URxvt.perl-lib: /home/pinusc/.urxvt/ext

URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.keysym.C-equal:  font-size:reset
URxvt.keysym.C-slash:  font-size:show

*.foreground:   base05
*.background:   base00
*.cursorColor:  base05

*.color0:       base00
*.color1:       base08
*.color2:       base0B
*.color3:       base0A
*.color4:       base0D
*.color5:       base0E
*.color6:       base0C
*.color7:       base05

*.color8:       base03
*.color9:       base08
*.color10:      base0B
*.color11:      base0A
*.color12:      base0D
*.color13:      base0E
*.color14:      base0C
*.color15:      base07

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
*.color16:      base09
*.color17:      base0F
*.color18:      base01
*.color19:      base02
*.color20:      base04
*.color21:      base06

rofi.color-enabled: false
rofi.color-window: base00, base00
rofi.color-normal: base00, base05, base00, base01, base05
rofi.color-active: base00, base05, base00, base01, base05
rofi.color-urgent: base00, base05, base00, base01, base05
rofi.monitor: -4
rofi.matching: fuzzy
rofi.font: Source Code Pro 11
